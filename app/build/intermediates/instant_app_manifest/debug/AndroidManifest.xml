        <activity android:name="com.android.SmartParking.Slot" />
        <activity android:name="com.android.SmartParking.CancelBooking" />
        <activity android:name="com.android.SmartParking.Feedback" />
        <activity android:name="com.android.SmartParking.ViewBooking" />
        <activity
            android:name="com.android.SmartParking.ChoosePlot"
            android:label="@string/app_name" />
        <activity
            android:name="com.android.SmartParking.MapActivity"
            android:label="@string/app_name" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyDbviX2weIX__UzIsOg-S5Mi4VeXAkOOoI" />

        <activity
